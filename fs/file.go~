package fs

import (
	"io"
	io_fs "io/fs"
)

type PMTileFile struct {
	io_fs.File
	reader io.Reader
	info   *PMTileFileInfo
}

func (f *PMTilesFile) Stat() (io_fs.FileInfo, error) {
	return f.info
}

func (f *PMTilesFile) Read(b []byte) (int, error) {
	return f.reader.Read(b)
}

func (f *PMTilesFile) Close() error {
	return nil
}
